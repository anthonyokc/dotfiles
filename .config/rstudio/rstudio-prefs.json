{
    "initial_working_directory": "~",
    "graphics_backend": "cairo-png",
    "auto_detect_indentation": true,
    "insert_native_pipe_operator": true,
    "highlight_selected_line": true,
    "relative_line_numbers": true,
    "show_invisibles": true,
    "rainbow_parentheses": true,
    "show_help_tooltip_on_idle": true,
    "code_completion_delay": 25,
    "editor_keybindings": "vim",
    "syntax_color_console": true,
    "console_line_length_limit": 2000,
    "editor_theme": "Oceanic - Eighties",
    "panes": {
        "quadrants": [
            "Source",
            "TabSet1",
            "Console",
            "TabSet2"
        ],
        "tabSet1": [
            "Environment",
            "Presentation"
        ],
        "tabSet2": [
            "Files",
            "Plots",
            "Help",
            "Viewer",
            "Presentations"
        ],
        "hiddenTabSet": [
            "History",
            "Connections",
            "Packages",
            "Build",
            "VCS",
            "Tutorial",
            "Presentations"
        ],
        "console_left_on_top": true,
        "console_right_on_top": true,
        "additional_source_columns": 0
    },
    "visual_markdown_editing_show_margin": true,
    "visual_markdown_code_editor_line_numbers": true,
    "pdf_previewer": "system",
    "posix_terminal_shell": "zsh",
    "show_hidden_files": true,
    "jobs_tab_visibility": "shown",
    "default_project_location": "~/ojo",
    "rmd_viewer_type": "pane",
    "default_sweave_engine": "knitr",
    "rmd_chunk_output_inline": false,
    "highlight_r_function_calls": true,
    "save_workspace": "never",
    "load_workspace": false,
    "auto_append_newline": true,
    "strip_trailing_whitespace": true,
    "show_diagnostics_other": true,
    "source_with_echo": true,
    "copilot_enabled": true,
    "style_diagnostics": true,
    "cran_mirror": {
        "name": "USA (MO) [https]",
        "host": "Washington University, St. Louis, MO",
        "url": "https://cran.wustl.edu/",
        "country": "us",
        "ok": 1,
        "secondary": ""
    },
    "copilot_indexing_enabled": true
}